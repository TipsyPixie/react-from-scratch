  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "http-errors": "^1.6.3",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "serve-favicon": "^2.5.0"
