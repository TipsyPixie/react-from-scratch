{
  "name": "react-from-scratch",
  "version": "1.0.0",
  "description": "React from scratch",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "npm run babel:build",
    "babel:build": "./node_modules/.bin/babel ./src --out-dir ./dist",
    "parcel:watch": "./node_modules/.bin/parcel watch src/ -d ./dist",
    "dev": "nodemon -x node ./dist/server/index.js",
    "start": "node ./dist/server/index.js"
  },
