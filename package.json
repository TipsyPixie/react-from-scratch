    "url": "https://github.com/TipsyPixie/react-from-scratch/issues"
  },
  "homepage": "https://github.com/TipsyPixie/react-from-scratch#readme",
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "http-errors": "^1.6.3",
    "parcel-bundler": "^1.9.7",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.76.0",
